[
  {
          "PropertyGroupId": "kinesis.analytics.flink.run.options",
          "PropertyMap": {
            "python": "TumblingWindows/tumbling-windows.py",
            "jarfile": "TumblingWindows/lib/amazon-kinesis-sql-connector-flink-2.0.3.jar"
          }
        },
        {
          "PropertyGroupId": "consumer.config.0",
          "PropertyMap": {
            "input.stream.name": "KinesisAnalyticsPenTestInputStream",
            "flink.stream.initpos": "LATEST",
            "aws.region": "us-east-1"
          }
        },
  {
    "PropertyGroupId": "producer.config.0",
    "PropertyMap": {
      "output.stream.name": "KinesisAnalyticsPenTestOutputStream",
      "shard.count": "1",
      "aws.region": "us-east-1"
    }
  }]